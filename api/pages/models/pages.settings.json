{
  "kind": "collectionType",
  "collectionName": "pages",
  "info": {
    "name": "pages",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "label": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "label"
    },
    "path": {
      "type": "string"
    },
    "pageSEO": {
      "type": "component",
      "repeatable": false,
      "component": "shared.seo"
    },
    "pageStructure": {
      "type": "dynamiczone",
      "components": [
        "sections.media-block",
        "sections.deck-block",
        "sections.text-block",
        "sections.hero-block",
        "layouts.article-layout",
        "sections.search-sort-block",
        "sections.contact-form-block"
      ]
    },
    "homePage": {
      "type": "boolean",
      "default": false,
      "unique": false
    },
    "keywords": {
      "via": "pages",
      "collection": "keywords"
    },
    "parent_page": {
      "model": "pages"
    },
    "static_path": {
      "type": "json"
    },
    "article_template": {
      "via": "page",
      "model": "article"
    }
  }
}
